{
  "manifest_version": 3,
  "name": "Coursera Helper Pro",
  "description": "This extension will help you to answer quiz and submit grade.",
  "version": "1.0",
  "permissions": ["activeTab", "scripting", "storage", "webRequest"],
  "icons": {
    "48": "assets/icon.png",
    "128": "assets/icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "assets/icon.png"
  },
  "host_permissions": ["https://www.coursera.org/*"]
}
